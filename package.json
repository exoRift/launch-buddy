{
  "name": "launch-buddy",
  "version": "1.0.0",
  "description": "LaunchBuddy is a tool to assist with syncing deployments, helping you discretely launch features in unison",
  "author": "exoRift",
  "bin": {
    "lb": "bin/lb"
  },
  "files": [
    "bin/"
  ],
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build src/index.ts --compile --outfile lb && mkdir -p bin && mv lb bin/lb",
    "lint": "eslint",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "inquirer": "^9.2.10"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.47.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "typescript": "^5.1.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/exoRift/launch-buddy.git"
  }
}
